        <Key android:codes="-59"   android:keyIcon="@drawable/ic_camel" /><!-- android:keyLabel="🐪" -->
        <Key android:codes="-1005" android:keyLabel="- ⌴" />
        <Key android:codes="-1003" android:keyLabel="_ ⌴" />
        <Key android:codes="-1006" android:keyLabel="⌴ -" />
        <Key android:codes="-1004" android:keyLabel="⌴ _" />
        <Key android:codes="-104"  android:keyLabel="#?" />
    </Row>
    <Row android:keyWidth="10%p" android:keyHeight="5.6%p">
        <Key android:codes="-1013" android:keyLabel="⌤" />
        <Key android:codes="-1014" android:keyLabel="⊼" />
        <Key android:codes="-1007" android:keyLabel="⌴ \\n" />
        <Key android:codes="-1008" android:keyLabel="\\n ⌴" />
        <Key android:codes="-1009" android:keyLabel="⌴ \\t" />
        <Key android:codes="-1010" android:keyLabel="\\t ⌴" />
        <Key android:codes="-1011" android:keyLabel="(.)\\n" />
        <Key android:codes="-1012" android:keyIcon="@drawable/ic_join" />
        <Key android:codes="-1015" android:keyLabel="⌴$" />
        <Key android:codes="-1016" android:keyLabel="\w⌴$" />
    </Row>
    <Row android:keyWidth="10%p" android:keyHeight="5.6%p">
        <Key android:codes="-48" android:keyIcon="@drawable/ic_indent" />
        <Key android:codes="-49" android:keyIcon="@drawable/ic_unindent" />
        <Key android:codes="-97" android:keyIcon="@drawable/ic_date" />
        <Key android:codes="-98" android:keyIcon="@drawable/ic_time" />
        <Key android:codes="0"   android:keyOutputText="@string/name"    android:keyIcon="@drawable/ic_name" />
        <Key android:codes="0"   android:keyOutputText="@string/email"   android:keyIcon="@drawable/ic_email" />
        <Key android:codes="0"   android:keyOutputText="@string/phone"   android:keyIcon="@drawable/ic_phone" />
        <Key android:codes="0"   android:keyOutputText="@string/address" android:keyIcon="@drawable/ic_address" />
        <Key android:codes="-105"  android:keyIcon="@drawable/ic_sort_lines" />
        <Key android:codes="-106"  android:keyIcon="@drawable/ic_reverse_lines" />
    </Row>
    <Row android:keyWidth="10%p" android:keyHeight="5.6%p">
        <Key android:codes="-5"  android:keyIcon="@drawable/ic_backspace" android:isRepeatable="true" android:horizontalGap="80%p" />
        <Key android:codes="-7"  android:keyIcon="@drawable/ic_delete"    android:isRepeatable="true" />
    </Row>
    <Row>
        <Key android:codes="-101" android:keyIcon="@drawable/ic_double_arrow_left"  android:keyWidth="10%p" />
        <Key android:codes="-102" android:keyIcon="@drawable/ic_double_arrow_right" android:keyWidth="10%p" />
        <Key android:codes="-499" android:keyIcon="@drawable/ic_keyboard"           android:keyWidth="10%p" />
        <Key android:codes="32"   android:keyIcon="@drawable/ic_space"              android:keyWidth="40%p" android:popupCharacters="@string/popup1" android:popupKeyboard="@layout/popup_template" />
        <Key android:codes="7"    android:keyIcon="@drawable/ic_tab"                android:keyWidth="10%p" android:popupCharacters="@string/popup2" android:popupKeyboard="@layout/popup_template" />
        <Key android:codes="10"   android:keyIcon="@drawable/ic_enter"              android:keyWidth="20%p" android:popupCharacters="@string/popup3" android:popupKeyboard="@layout/popup_template" />
    </Row>
</Keyboard>
