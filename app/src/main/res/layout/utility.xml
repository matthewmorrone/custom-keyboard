    </Row>
    <Row>
        <Key android:keyLabel="\\\\" android:keyOutputText="\\\\" />
        <Key android:keyOutputText="\\n" android:keyLabel="\\n" />
        <Key android:keyOutputText="\\t" android:keyLabel="\\t" />
        <Key android:keyLabel="\\s" android:keyOutputText="\\s" android:popupCharacters="S" android:popupKeyboard="@layout/popup_template" />
        <Key android:keyLabel="\\w" android:keyOutputText="\\w" android:popupCharacters="W" android:popupKeyboard="@layout/popup_template" />
