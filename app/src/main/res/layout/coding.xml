<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:keyWidth="10%p"
          android:keyHeight="7%p">
    <Row android:keyWidth="8.333%p">
        <Key android:codes="-20" android:keyIcon="@drawable/ic_double_pointer_left" />
        <Key android:codes="-21" android:keyIcon="@drawable/ic_double_pointer_right" />
        <Key android:codes="-13" android:keyIcon="@drawable/ic_arrow_up" />
        <Key android:codes="-14" android:keyIcon="@drawable/ic_arrow_down" />
        <Key android:codes="-15" android:keyIcon="@drawable/ic_arrow_left" />
        <Key android:codes="-16" android:keyIcon="@drawable/ic_arrow_right" />
        <Key android:codes="-8"  android:keyIcon="@drawable/ic_cut" />
        <Key android:codes="-9"  android:keyIcon="@drawable/ic_copy" />
        <Key android:codes="-10" android:keyIcon="@drawable/ic_paste" />
        <Key android:codes="-11" android:keyIcon="@drawable/ic_select" />
        <Key android:codes="-12" android:keyIcon="@drawable/ic_select_all" />
        <Key android:codes="-23" android:keyIcon="@drawable/ic_voice" />
    </Row>
    <Row>
        <Key android:keyLabel="true" android:keyOutputText="true" />
        <Key android:keyLabel="false" android:keyOutputText="false" />
        <Key android:keyLabel="null" android:keyOutputText="null" />
        <Key android:keyLabel="if" android:keyOutputText="if () {}" />
        <Key android:keyLabel="else" android:keyOutputText="else {}" />

        <Key android:keyLabel="for" android:keyOutputText="for (int i = 0; i &lt; .length(); i++) {}" />
        <Key android:keyLabel="each" android:keyOutputText="for ( : ) {}" />
        <Key android:keyLabel="try" android:keyOutputText="try {}" />
        <Key android:keyLabel="catch" android:keyOutputText="catch(Exception e) {}" />
        <Key android:keyLabel="print" android:keyOutputText="System.out.println();" />
    </Row>
    <Row>
        <Key android:keyLabel="switch" android:keyOutputText="switch () {case &quot;&quot;: {}; default: {}};}" />
        <Key android:keyLabel="length" android:keyOutputText=".length()" />
        <Key android:keyLabel="size" android:keyOutputText=".size()" />
        <Key android:keyLabel="\?:" android:keyOutputText="(  ==  ?  : &quot;&quot;)" />
        <Key android:keyLabel="()" android:keyOutputText="()" />

        <Key android:keyLabel="[]" android:keyOutputText="[]" />
        <Key android:keyLabel="{}" android:keyOutputText="{}" />
        <Key android:keyLabel="&lt;&gt;" android:keyOutputText="&lt;&gt;" />
        <Key android:keyLabel="''" android:keyOutputText="''" />
        <Key android:keyLabel="&quot;&quot;" android:keyOutputText="&quot;&quot;" />
    </Row>
    <Row>
        <Key android:keyLabel="&amp;&amp;" android:keyOutputText="&amp;&amp;" />
        <Key android:keyLabel="||" android:keyOutputText="||" />
        <Key android:keyLabel="==" android:keyOutputText=" == " android:popupCharacters="⩵⩶" android:popupKeyboard="@layout/popup_template" />
        <Key android:keyLabel="!=" android:keyOutputText=" != " />
        <Key android:keyLabel="--" android:keyOutputText="--" />

        <Key android:keyLabel="++" android:keyOutputText="++" />
        <Key android:keyLabel="+' '+" android:keyOutputText="+' '+" />
        <Key android:keyLabel="+&quot; &quot;+" android:keyOutputText="+&quot; &quot;+" />
        <Key android:keyLabel="&lt;!--" android:keyOutputText="&lt;!--&#32;" />
        <Key android:keyLabel="--&gt;" android:keyOutputText="&#32;--&gt;" />
    </Row>
    <Row>
        <Key android:keyLabel="//&#32;" android:keyOutputText="//&#32;" />
        <Key android:keyLabel="/*" android:keyOutputText="/*" />
        <Key android:keyLabel="*/" android:keyOutputText="*/" />
        <Key android:codes="-168" android:keyIcon="@drawable/ic_format_unindent" />
        <Key android:codes="-169" android:keyIcon="@drawable/ic_format_indent" />

        <Key android:codes="-170" android:keyLabel="&lt;\!--" />
        <Key android:codes="-171" android:keyLabel="/*" />
        <Key android:codes="-172" android:keyLabel="//" />
        <Key android:codes="-5" android:keyIcon="@drawable/ic_backspace" />
        <Key android:codes="-7" android:keyIcon="@drawable/ic_delete"    />
    </Row>
    <Row>
        <Key android:codes="-22"  android:keyIcon="@drawable/ic_settings"        android:keyWidth="12.5%p" />
        <Key android:codes="-101" android:keyIcon="@drawable/ic_swap"            android:keyWidth="12.5%p" />
        <Key android:codes="32"   android:keyIcon="@drawable/ic_space"           android:keyWidth="50%p" />
        <Key android:codes="10"   android:keyIcon="@drawable/ic_keyboard_return" android:keyWidth="25%p" />
    </Row>
</Keyboard>
